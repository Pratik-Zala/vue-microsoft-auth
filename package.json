{
  "name": "@twymai/vue-twymx",
  "version": "2.1.1",
  "description": "A Vue.js plugin for Microsoft authentication with OAuth2",
  "main": "dist/@twymai/vue-twymx.umd.js",
  "module": "dist/@twymai/vue-twymx.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/@twymai/vue-twymx.es.js",
      "require": "./dist/@twymai/vue-twymx.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css",
    "./styles-processed.css": "./dist/styles-processed.css",
    "./tailwind.config.js": "./tailwind.config.js"
  },
  "files": [
    "dist",
    "tailwind.config.js",
    "build-css.js"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "npm run typecheck && vite build && tsc --emitDeclarationOnly && npm run copy-components && npm run copy-styles && npm run build-css",
    "copy-components": "mkdir -p dist/components && cp -r src/components/*.vue dist/components/",
    "copy-styles": "cp src/styles.css dist/styles.css",
    "build-css": "node build-css.js",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "microsoft",
    "auth",
    "oauth",
    "authentication",
    "login",
    "signin"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "tailwindcss": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.0.0",
    "vite": "^4.5.14",
    "vue": "^3.3.0",
    "vue-router": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twymai/vue-twymx.git"
  },
  "bugs": {
    "url": "https://github.com/twymai/vue-twymx/issues"
  },
  "homepage": "https://github.com/twymai/vue-twymx#readme",
  "dependencies": {
    "axios": "^1.11.0",
    "vue-router": "^4.5.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
