{
  "name": "@twymai/vue-twymx",
  "version": "1.0.1",
  "description": "A Vue.js plugin for Microsoft authentication with OAuth2",
  "main": "dist/@twymai/vue-twymx.umd.js",
  "module": "dist/@twymai/vue-twymx.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly && npm run copy-components",
    "copy-components": "mkdir -p dist/components && cp -r src/components/*.vue dist/components/",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "microsoft",
    "auth",
    "oauth",
    "authentication",
    "login",
    "signin"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.5.14",
    "vue": "^3.3.0",
    "vue-router": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twymai/vue-twymx.git"
  },
  "bugs": {
    "url": "https://github.com/twymai/vue-twymx/issues"
  },
  "homepage": "https://github.com/twymai/vue-twymx#readme",
  "dependencies": {
    "axios": "^1.11.0",
    "vue-router": "^4.5.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
